<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.UDrawMapper">

    <!-- Generic Query Mapping Results -->
    <resultMap id="BaseResultMap" type="com.example.demo.entity.UDrawEntity">
        <id column="id" property="id" />
        <result column="u_id" property="uId" />
        <result column="g_id" property="gId" />
        <result column="to_id" property="toId" />
        <result column="answer" property="answer" />
        <result column="add_time" property="addTime" />
        <result column="path" property="path" />
        <result column="guess" property="guess" />
        <result column="tip" property="tip" />
    </resultMap>

    <!-- Generic query result column -->
    <sql id="Base_Column_List">
        id, u_id,g_id, to_id, answer, add_time, path, guess,tip
    </sql>

</mapper>
